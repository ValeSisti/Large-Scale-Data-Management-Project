      "field": {
        "fieldNames": ["track_id", "album_id"],
        "operation": "remove"
      }
    }
  ],
  "loader": {
    "orientdb": {
      "dbURL": "E:/OrientDBProject/orientdb-community-3.2.16/databases/bdm",
      "dbType": "graph",
      "dbUser": "root",
      "dbPassword": "lsdm2022",
      "serverUser": "root",
      "serverPassword": "lsdm2022",
      "tx": false,
      "batchCommit": 1000,
      "classes": [
        { "name": "isSongInAlbum", "extends": "E" },
        { "name": "Song", "extends": "V" },
        { "name": "Album", "extends": "V" }
      ]
    }
  }
}
