      "dbType": "graph",
      "dbUser": "root",
      "dbPassword": "lsdm2022",
      "serverUser": "root",
      "serverPassword": "lsdm2022",
      "tx": false,
      "batchCommit": 1000,
      "classes": [{ "name": "Lyrics", "extends": "V" }],
      "indexes": [
        { "class": "Lyrics", "fields": ["lyrics:String"], "type": "UNIQUE" }
      ]
    }
  }
}
